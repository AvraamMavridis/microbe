{
  "name": "microbe.js",
  "version": "0.3.4",
  "author": "Mouse Braun <mouse@sociomantic.com>",
  "description": "micro-library front-end",
  "contributors": [
    {
      "name": "Nicolas Brugneaux",
      "email": "nicolas.brugneaux@sociomantic.com"
    },
    {
      "name": "Michele Guido",
      "email": "michele.guido@sociomantic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sociomantic/microbe"
  },
  "scripts": {
    "get-dependencies": "npm install . && echo \"Installation complete.\"",
    "build": "./node_modules/.bin/gulp && echo \"Build complete.\"",
    "test": "./node_modules/.bin/gulp test && echo \"Tests complete.\"",
    "watch": "./node_modules/.bin/gulp watch",
    "buildDoc": "cd src && ../node_modules/.bin/docker -o ../www/doc/ -i ./ --sidebar true --watch --js ../dist/microbe.js --extras fileSearch -c manni",
    "serve": "cd www && python -m SimpleHTTPServer 8000"
  },
  "devDependencies": {
    "browserify": "^5.11.2",
    "docker": "git://github.com/nicolasbrugneaux/docker.git#patch-1",
    "gulp": "^3.8.7",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.4.0",
    "gulp-rimraf": "^0.1.0",
    "gulp-uglify": "^1.2.0",
    "observe-shim": "^0.4.1",
    "observe-utils": "^0.3.2",
    "promise": "^6.0.0",
    "qunitjs": "^1.18.0",
    "setimmediate": "^1.0.2"
  },
  "license": "MIT"
}
